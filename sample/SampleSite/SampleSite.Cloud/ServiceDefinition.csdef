<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="SampleSite.Cloud" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-10.1.8">
    <WebRole name="SampleSite" vmsize="Small">
        <Sites>
            <Site name="Web">
                <Bindings>
                    <Binding name="Http" endpointName="Http" />
                </Bindings>
            </Site>
        </Sites>
        <ConfigurationSettings>
            <Setting name="ItsASecret" />
        </ConfigurationSettings>
        <Endpoints>
            <InputEndpoint name="Http" protocol="http" port="80" />
        </Endpoints>
        <Imports>
            <Import moduleName="Diagnostics" />
        </Imports>
    </WebRole>
</ServiceDefinition>